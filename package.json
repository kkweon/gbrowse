{
  "name": "gbrowse",
  "version": "2.0.1",
  "description": "Open GitHub in Browser",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "repository": "https://github.com/kkweon/gbrowse",
  "homepage": "https://github.com/kkweon/gbrowse#readme",
  "keywords": [
    "github",
    "git",
    "utils",
    "browse",
    "open"
  ],
  "bugs": {
    "url": "https://github.com/kkweon/gbrowse/issues",
    "email": "kkweon@gmail.com"
  },
  "author": "Mo Kweon",
  "license": "MIT",
  "private": false,
  "bin": "./bin/gbrowse",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project .",
    "format": "prettier --write \"*.{ts,js,json}\" \"src/**/*.{ts,js,json}\"",
    "test": "npm run lint && tsc --noEmit && nyc mocha \"src/**/*.spec.ts\"",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepare": "npm run build"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "devDependencies": {
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/open": "^6.2.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.7",
    "mocha": "^6.2.2",
    "prettier": "^1.18.2",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "nyc": "^14.1.1",
    "open": "^7.0.0"
  }
}
